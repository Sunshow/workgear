{
  "name": "workgear",
  "version": "0.1.0",
  "private": true,
  "description": "AI Agent 工作流编排平台",
  "scripts": {
    "dev": "pnpm run dev:web & pnpm run dev:api & pnpm run dev:orchestrator",
    "dev:web": "pnpm --filter @workgear/web dev",
    "dev:api": "pnpm --filter @workgear/api dev",
    "dev:orchestrator": "cd packages/orchestrator && make run",
    "build": "pnpm --recursive build",
    "clean": "pnpm --recursive clean"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.28.2"
}
